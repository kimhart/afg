<!DOCTYPE html>
<html lang="en" ng-app="modelApp">
<head>
  <title>AFG</title>
  <meta name="viewport" content="width=device-width, initial-scale=1">
  <link rel="stylesheet" type="text/css" href="css/skeleton.css">
  <link rel="stylesheet" type="text/css" href="css/style.css">
  <link href='https://fonts.googleapis.com/css?family=Raleway:400,300' rel='stylesheet' type='text/css'>
</head>
<body>
<div ng-controller="ImagesController">  
  <div class="container">
    <div class="content">
    <h1>Art & Fashion Group</h1>
      <div class="grid">
          <div class="row">
            <div ng-repeat="image in images">
              <div class="one-third column tilt"><img class="model-image" ng-src="{{image.src}}">
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
  <img class="footer-img" src="img/skyline.png">
</body>
<script type="text/javascript" src="bower_components/jquery/dist/jquery.js"> </script>
<script src="bower_components/angular/angular.js"></script>

<script src="js/app.js"></script>
<script src="js/images_controller.js"></script>
</html>